{
  "name": "@dupenet/physics",
  "version": "0.0.1",
  "description": "Frozen physics: canonical encoding, hash rules, CID semantics, schemas, merkle",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:vectors": "vitest run --reporter=verbose test/vectors",
    "test:unit": "vitest run --reporter=verbose test/unit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@noble/hashes": "^1.7.1",
    "@sinclair/typebox": "^0.34.14",
    "cbor-x": "^1.6.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
